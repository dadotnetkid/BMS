
@{
    ViewBag.Title = "Dashboard";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="row">
    @Html.Action("PersonalInformationPartial")
    <!-- ./col -->
    @Html.Action("MeetingsPartial")
    <!-- ./col -->
    @Html.Action("ActivitiesPartial")
    <!-- ./col -->
    @Html.Action("BlottersPartial")
    <!-- ./col -->
</div>
<div class="row">
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class=" panel-heading">
                <h4>Residents Summary</h4>
            </div>
            <div class="panel-body">
                @Html.Action("PersonalInformationGridViewPartial")

            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class=" panel-heading">
                <h4>Meetings Summary</h4>
            </div>
            <div class="panel-body">
                @Html.Action("MeetingGridViewPartial")

            </div>
        </div>
    </div>
</div>
<div class="row">
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class=" panel-heading">
                <h4>Blotters Summary</h4>
            </div>
            <div class="panel-body">
                @Html.Action("BlottersGridViewPartial")

            </div>
        </div>
    </div>
    <div class="col-lg-6">
        <div class="panel panel-default">
            <div class=" panel-heading">
                <h4>Activities Summary</h4>
            </div>
            <div class="panel-body">
                @Html.Action("ActivityGridViewPartial")

            </div>
        </div>
    </div>
</div>
